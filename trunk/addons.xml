<?xml version="1.0" encoding="ISO-8859-1"?>
<addons>
  <!-- Australian Catchup TV Plugins -->
  <addon
  id="plugin.video.sbs2"
  name="SBS2"
  version="1.0.2"
  provider-name="Denis">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="video"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension 
    point="xbmc.addon.metadata"
    library="default.py">
    <summary>SBS 2</summary>
    <description>Catch up TV content for SBS</description>
    <provides>video</provides>
    <platform>all</platform>
	<disclaimer>You must be within Australia to use watch these
      videos.[CR]"SBS", SBS Logos and other works are copyright of
      their respective owners.</disclaimer>	
  </extension>
</addon>
  <addon id="plugin.video.catchuptv.au.nten" name="Network Ten CatchUp TV" version="0.2.0" provider-name="adammw111">
    <requires>
        <import addon="xbmc.python" version="1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <minversion>31542</minversion>
        <summary>Network Ten CatchUp TV Video Player</summary>
        <description>Watch latest episodes, sneak peeks, exclusive internet extras and other videos from Network Ten Online.</description>
        <disclaimer>You must be within Australia to use watch these videos.[CR]"Network Ten", Ten Logos and other works are copyright of their respective owners.</disclaimer>
    </extension>
</addon>
